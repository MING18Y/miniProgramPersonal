.home_title_bar {
  height: 72rpx;
  background: #ffffff;
  position: relative;
  padding-left: 20rpx;
  box-sizing: border-box;
  line-height: 72rpx;
  color: #333333;
  font-size: 30rpx;
}
.home_title_bar .more {
  position: absolute;
  right: 20rpx;
  top: 0;
  display: flex;
  align-items: center;
  color: #999999;
  font-size: 30rpx;
}
.home_title_bar .more .more-icon {
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}
